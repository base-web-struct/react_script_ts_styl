.cooperate-main
  width: 100%;
  height 100%;
  display: flex;
  align-items: center;
  justify-content: center;

  .cooperate-con
    width: 100%;
    max-width: 1200px;
    height calc(100% - 30px);
    background-color: rgba(4, 50, 100, .51);
    border: 1px solid rgba(11, 141, 246, .3);
    padding: 15px;

    .coo-header
      width: 100%;
      height: 36px;
      border-bottom: 1px solid rgba(255, 255, 255, .3);
      display: flex;
      align-items: flex-start;
      & > i 
        display: block;
        width: 4px;
        height: 24px;
        background-color: #299DF2;
        margin-right: 10px;

      & > span 
        font-size: 16px;
        color: rgba(255, 255, 255, .7);

    .coo-body
      width: 100%;
      height calc(100% - 41px);
      padding: 15px;

      .table-con
        width: 100%;
        margin-top: 18px;
        height calc(100% - 50px);

        .ant-table-wrapper
          width: 100%;
          height 100%;
