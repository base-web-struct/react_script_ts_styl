.home-main
  width: 100%;
  height 100%;
  position: relative;

  .home-frame
    width: 100%;
    height 100%;
    border: 0;

    &.full-screen 
      filter: invert(1) saturate(0.8) hue-rotate(290deg);

  .full-screen-btn 
    position: absolute;
    top: 10px;
    right: 10px;
    color: #fff;
    font-size: 16p;
    font-weight: 600;
    border: 1px solid rgb(103, 185, 245);
    padding: 5px;  
    cursor: pointer;
    background-color: #122c68;
    border-radius: 5px;